{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.selectionHighlight": true,
    "editor.suggest.snippetsPreventQuickSuggestions": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "matchingDocuments"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.autoIndent": "advanced",
    "editor.colorDecorators": true,
    "editor.cursorStyle": "line",
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "a-file-icon-vscode.activeIconPacks": [
    "angular",
    "angular2",
    "ngrx",
    "nest",
    "next",
    "react",
    "redux",
    "recoil",
    "vue",
    "vuex",
    "rails",
    "phalcon",
    "volt",
    "none"
  ],
  "a-file-icon-vscode.arrowTheme": "triangle",
  "a-file-icon-vscode.folderTheme": "specific",
  "better-comments.tags": [
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#FF0000",
      "italic": true,
      "strikethrough": false,
      "tag": "!",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#1E90FF",
      "italic": true,
      "strikethrough": false,
      "tag": "?",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#808080",
      "italic": false,
      "strikethrough": false,
      "tag": "//",
      "underline": false
    },
    {
      "bold": true,
      "color": "#009534",
      "italic": true,
      "strikethrough": false,
      "tag": "*",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#FF8C00",
      "italic": true,
      "strikethrough": false,
      "tag": "todo",
      "underline": false
    },
    {
      "bold": false,
      "color": "#FEDE00",
      "italic": true,
      "strikethrough": false,
      "tag": "tip",
      "underline": false
    }
  ],
  "code-runner.clearPreviousOutput": true,
  "code-runner.executorMap": {
    "ahk": "autohotkey",
    "applescript": "osascript",
    "autoit": "autoit3",
    "bat": "cmd /c",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "clojure": "lein exec",
    "coffeescript": "coffee",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "crystal": "crystal",
    "csharp": "scriptcs",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "dart": "dart",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fsharp": "fsi",
    "go": "go run",
    "groovy": "groovy",
    "haskell": "runhaskell",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "javascript": "node",
    "julia": "julia",
    "kit": "kitc --run",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "lisp": "sbcl --script",
    "lua": "lua",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "ocaml": "ocaml",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "perl": "perl",
    "perl6": "perl6",
    "php": "php",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "python": "set PYTHONIOENCODING=utf8 && python",
    "r": "Rscript",
    "racket": "racket",
    "ruby": "ruby",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "sass": "sass --style expanded",
    "scala": "scala",
    "scheme": "csi -script",
    "scss": "scss --style expanded",
    "shellscript": "bash",
    "swift": "swift",
    "typescript": "ts-node",
    "v": "v run",
    "vbscript": "cscript //Nologo"
  },
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "console-ninja.featureSet": "Community",
  "console-ninja.toolsToEnableSupportAutomaticallyFor": {
    "live-preview-extension": true,
    "live-server-extension": true
  },
  "database-client.autoSync": true,
  "debug.showInlineBreakpointCandidates": false,
  "debug.terminal.clearBeforeReusing": true,
  "diffEditor.codeLens": true,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingOvertype": "always",
  "editor.autoClosingQuotes": "always",
  "editor.autoIndent": "advanced",
  "editor.autoSurround": "brackets",
  "editor.bracketPairColorization.enabled": true,
  "editor.colorDecorators": true,
  "editor.cursorBlinking": "blink",
  "editor.cursorStyle": "underline",
  "editor.defaultColorDecorators": "always",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrains Mono, Martian Mono, Spline Sans Mono, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.inlineSuggest.enabled": true,
  "editor.letterSpacing": 0.3,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorPaste": "full",
  "editor.renderControlCharacters": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.selection": "first",
  "editor.suggest.selectionMode": "always",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emulator.emulatorPath": "C:/Users/pedro/AppData/Local/Android/Sdk/emulator",
  //! "emulator.emulatorPathWindows": "<yourAndroidHome>\\Sdk\\emulator",
  "emulator.emulatorPathMac": "~/Library/Android/sdk/emulator",
  "emulator.emulatorPathWindows": "C:/Users/pedro/AppData/Local/Android/Sdk/emulator",
  //! "emulator.emulatorPathLinux": "~/Android/Sdk/emulator",
  "explorer.compactFolders": false,
  "explorer.confirmDelete": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.db": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.py": "",
    "*.s3db": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "tsconfig.json": "tsconfig.*.json"
  },
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 10,
  "files.associations": {
    "*.py": "python"
  },
  "files.exclude": {
    "**/.exe": true
  },
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true
  },
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4o",
  "hediet.vscode-drawio.resizeImages": null,
  "html.format.indentHandlebars": true,
  "javascript.format.placeOpenBraceOnNewLineForControlBlocks": true,
  "javascript.format.placeOpenBraceOnNewLineForFunctions": true,
  "javascript.validate.enable": true,
  "liveServer.settings.donotShowInfoMsg": false,
  "liveServer.settings.donotVerifyTags": true,
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.enableLogging": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "prisma.showPrismaDataPlatformNotification": true,
  "redhat.telemetry.enabled": true,
  "settingsSync.ignoredSettings": [
    "extensions.allowed",
    "extensions.autoUpdate",
    "extensions.autoCheckUpdates",
    "extensions.showRecommendationsOnlyOnDemand"
  ],
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.env.osx": {},
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontFamily": "Roboto Mono, Fragment Mono, Space Mono, monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.persistentSessionReviveProcess": "onExit",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Aura Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.iconTheme": "a-file-icon-vscode",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none"
}
